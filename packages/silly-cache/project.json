{
  "root": "packages/silly-cache",
  "sourceRoot": "packages/silly-cache/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/silly-cache",
        "main": "packages/silly-cache/src/index.ts",
        "tsConfig": "packages/silly-cache/tsconfig.lib.json",
        "assets": ["packages/silly-cache/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/silly-cache/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/silly-cache"],
      "options": {
        "jestConfig": "packages/silly-cache/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
